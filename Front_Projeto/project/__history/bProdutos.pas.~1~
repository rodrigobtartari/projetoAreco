unit bProdutos;

interface

uses
  pProdutos,
  System.SysUtils;

type
  TProdutos = Class(TObject)
    pProd : TfrmProdutos;
  public
    constructor create;
  End;

implementation

{ TProdutos }

constructor TProdutos.create;
begin
  pProd := TfrmProdutos.Create(nil);
  pProd.ShowModal;
end;

end.
